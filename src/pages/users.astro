---
export const prerender = true;

const response = await fetch('https://demo.halimlardjane.com/ci-rest/api/users');
const users = await response.json();
users.length = 5

---

<h1>My users</h1>

{
  users.map((user:any) => {
   return (
   <article class="card">
      <div>
        <h3>
          <span>{user.first_name}</span>
          <span>{user.last_name}</span>
        </h3>
      </div>
    </article>
   )
  })
}


<style>
  .card {
    margin: 10px 0;
    background-color: papayawhip;
  }
</style>
